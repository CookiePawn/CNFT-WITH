                        <span class="pt5">&nbsp;작성일</span>
                    </div>
                    <ul>
                        <% for (var i = (list.length-1); i >= 0; i--)  { %>
                            <% var comCnt=0;
                            for(var j = 0; j < clist.length; j++) {
                                if(list[i].postNum == clist[j].postNum) {
                                    comCnt++;
                                } 
                            }%>
                            <form method="get" id="postForm postFormID<%= i%>" action="<%=postInfo%>">
                                <a type="submit" onclick="document.getElementById('postForm postFormID<%= i%>').submit();">
                                    <div class="postListInfoDiv">
                                        <span class="plt1">&nbsp;<%= list[i].postNum %></span>
                                        <%if(list[i].img == "" && comCnt != 0) {%>
                                            <span class="plt2">&nbsp;<%= list[i].title.substr(0, 20) %>&nbsp;<span>[<%=comCnt%>]</span></span>
                                        <%} else if(list[i].img == "" && comCnt == 0) {%>
                                                <span class="plt2">&nbsp;<%= list[i].title.substr(0, 20) %></span>
                                        <%} else if(list[i].img != "" && comCnt != 0) {%>
                                            <span class="plt2">&nbsp;<%= list[i].title.substr(0, 20) %>&nbsp;<span>[<%=comCnt%>]</span>&nbsp;&nbsp;📷</span>
                                        <%} else if(list[i].img != "" && comCnt == 0) {%>             
                                            <span class="plt2">&nbsp;<%= list[i].title.substr(0, 20) %>&nbsp;&nbsp;📷</span>                                   
                                        <%}%>
                                        <span class="plt3">&nbsp;
                                            <img src="<%=list[i].authorIcon%>" width="30px">
                                            &nbsp;<%= list[i].author %>
                                        </span>
                                        <span class="plt4">&nbsp;<%= list[i].hit %></span>
                                        <%list[i].DATE = list[i].DATE.substr(0, 10)%>
                                        <span class="plt5">&nbsp;<%= list[i].DATE %></span>
                                        <input name="postID" type="hidden" value="<%= list[i].postNum%>">
                                    </div>
                                </a>
                            </form>
                        <%}%>
                    </ul>
