                        <div name="comTitle">
                            <span>댓글</span>&nbsp;&nbsp;&nbsp;총 <%=list2.length%>개
                            <form method="post" name="comPostForm" onsubmit="return comPost()">
                                <input type="hidden" id="nicknameID" name="nickname">
                                <input type="hidden" id="iconID" name="icon">
                                <input type="hidden" id="cEmailID" name="cEmail">
                                <input type="hidden" id="authorID" name="comAuthor" value="0">
                                <div name="comPosting">
                                    <textarea name="comContent" placeholder="댓글을 입력해주세요"></textarea>
