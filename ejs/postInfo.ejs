                        </div>
                        <% var list2st = JSON.stringify(list2);%>
                        <% list2st = list2st.replace(/\\r/gi, '').replace(/\\n/gi, '<br>');%>
                        <% var parseList = JSON.parse(list2st);%>
    
                        <%for(var i=parseList.length-1; i>=0; i--) {%>
                            <form method="post" id="comForm<%=i%>">
                                <input type="hidden" name="comNum" value="<%=parseList[i].comNum%>">
                                <input type="hidden" id="emailID2<%=i%>" name="comEmail2" value="<%=parseList[i].email%>">
                                <div name="comContent">
                                    <p>
                                        <img src="<%=parseList[i].userIcon%>" width="32px" height="32px">
                                        <%if(parseList[i].author == 1) {%>
                                            <span>&nbsp;&nbsp;<mark><%=parseList[i].user%></mark></span>
                                        <%} else {%>
                                            <span style="color: black;">&nbsp;&nbsp;<%=parseList[i].user%></span>
                                        <%}%>
                                        &nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;<span id="comDate<%=i%>"></span>
                                    </p>
                                    <script>
                                        document.getElementById('comDate<%=i%>').innerHTML = elapsedTime('<%=parseList[i].DATE%>');
                                    </script>
                                    <div class="comContentTextDiv <%=i%>">
                                        <p id="p<%=i%>"><%-parseList[i].content%></p>
                                        <textarea id="textarea<%=i%>" name="comContentEdit"><%-parseList[i].content%></textarea>
                                    </div>
                                    <form method="post">
                                        <div class="comBtnDiv">

                                        </div>
                                    </form>
                                </div>
                            </form>
                        <%}%>
